.host 
    h2.host-heading Popular Hosts 
    ul.host-list
        //- each user in [{name: "Thomas", color: "hsla(14, 100%, 82%, 1)"}, {name: "Edison", color: "hsla(39, 100%, 75%, 1)"}]
        - var n = 7;
        while n > 0 
            li.host-item 
                .host-avatar(style= "--color: hsla(14, 100%, 82%, 1)") 
                    img(srcSet="./images/hot-user.png 2x" alt="thomas")
                span.host-name= Thomas
            - n--
